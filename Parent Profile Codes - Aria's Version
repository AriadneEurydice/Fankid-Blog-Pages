      var dd = 1;  // how many images you have to change
      var cnt = 0;
      function chng() { 

          var rotator = document.getElementById('rr');  // change to match image ID
          var imageDir = '0';                          // change to match images folder 


          var images = [
          'https://64.media.tumblr.com/6910349eec22164e2dfcb779600b8dd1/a083ff89bc23f692-6c/s1280x1920/82b5db21cdadcc09e9a6554cc6bf729129383dde.png'];
          
          cnt++;

          var len = images.length;
          if (cnt < dd) {
              rr.src = images[cnt];
          }
          else if (cnt == len) {
              rr.src =images[0];
              cnt = 0;
          }
      }

</script>

  
