 <!--SCRIPTS
 DON'T MESS WITH THE SCRIPTS UNLESS YOU KNOW WHAT YOU'RE DOING-->
 <script>
   $(document).ready(function() {

        /*CCARD COLORS*/
        $('.ccard').each(function() {
            var groupClass = $(this).attr('class').split(" ")[1];

            $(this).find('.cell1').addClass(groupClass);
            $(this).find('.cell2').addClass(groupClass);
            $(this).find('.ctags').addClass(groupClass);
        });

        /*CARDS*/
        $('.cl').on('click', function(e) {
            var currentAttrValue = $(this).attr('href');
            var groupClass = $('.ccard' + currentAttrValue).attr('class').split(" ")[1];

            window.location.hash = $(this).attr('href');
            $('.ccard' + currentAttrValue).addClass('active').siblings('.ccard').removeClass('active');
           $(this).addClass('active').siblings('.cl').removeClass('active');
        });

        /*HASH SCRIPT*/
        var hash = $.trim(window.location.hash);

        if (hash) {
            $('.cl[href$="' + hash + '"]').trigger('click');
         };


        /*TOOLTIPS*/
        $(function() {
            $(".ccard").tooltip({
               classes: {
                 "ui-tooltip": "tooltip"},
               position: {
                 my: "right bottom",
                 at: "right top",
                 collision: "none"
               },
            });
         });

    }); //DOCUMENT READY

   </script>
 </body>
</html>
